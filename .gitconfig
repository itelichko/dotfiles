[user]
	name  = Ivan Telichko
	email = itelichko@gmail.com
[alias]
	co   = checkout
	ci   = commit
	st   = status
	br   = branch
	hist = log --pretty=format:\"%h %ad | %s%d [%an]\" --graph --date=short
	type = cat-file -t
	dump = cat-file -p
[core]
	editor = vim
	pager = vim -R -
[push]
	default = tracking
[merge]
	tool = vimdiff
[branch]
	autosetuprebase = always
[color]
	ui = auto
	diff = no
